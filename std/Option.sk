module Option {

pub enum Option[T] {
    None,
    Some(T),

    pub fn unwrap(self) -> T {
        match self {
            Some(v) -> v,
            None -> panic("Unwrap called on Option.None")
        }
    }

    pub fn isNone(&self) -> Bool {
        match self {
            None -> True,
            _ -> False,
        }
    }
}

}