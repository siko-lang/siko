module Std.Ops {

pub trait Add[T, U] {
    type Output
    fn add(self, other: U) -> Output
}

pub trait Sub[T] {
    fn sub(self, other: T) -> T
}

pub trait Mul[T] {
    fn mul(self, other: T) -> T
}

pub trait Div[T] {
    fn div(self, other: T) -> T
}

pub trait Not[T] {
    fn not(self) -> T
}

pub trait Neg[T] {
    fn negative(self) -> T
}

}