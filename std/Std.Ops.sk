module Std.Ops {

pub protocol Add[T, U] {
    type Output
    fn add(self, other: U) -> Output
}

pub protocol Sub[T, U] {
    fn sub(self, other: U) -> T
}

pub protocol Mul[T, U] {
    fn mul(self, other: U) -> T
}

pub protocol Div[T, U] {
    fn div(self, other: U) -> T
}

pub protocol Not[T] {
    fn not(self) -> T
}

pub protocol Neg[T] {
    fn negative(self) -> T
}

}