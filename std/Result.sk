module Result {

@derive(PartialEq, Eq, PartialOrd, Ord)
pub enum Result[T, E] {
    Ok(T),
    Err(E)

    fn unwrap(self) -> T {
        match self {
            Ok(v) -> v,
            Err(_) -> panic("Unwrap called on Result.Err")
        }
    }

    fn expect(self, msg: String) -> T {
        match self {
            Ok(v) -> v,
            Err(_) -> panic(msg)
        }
    }
}

}