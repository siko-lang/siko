module Libc {
    pub fn memcpy[T](dest: *T, src: *T, n: U64) -> *T = extern "C"

    pub fn memmove[T](dest: *T, src: *T, n: U64) -> *T = extern "C"

    pub fn memcmp[T](s1: *T, s2: *T, n: U64) -> I32 = extern "C"

    pub fn malloc[T](size: U64) -> *T = extern "C"

    pub fn free[T](ptr: *T) = extern "C"

    pub fn printf(format: *U8, value: *U8, len: Int) = extern "C" // TODO: implement varargs
}
