module Std.Ops.Basic {

pub trait Clone[T] {
    fn clone(&self) -> T
}

pub trait Copy[T]

pub trait Drop[T] {
    fn drop(self) -> Self
}

pub trait ImplicitConvert[T, U] {
    fn implicitConvert(self) -> U
}

pub trait Discriminator[T] {
    fn discriminator(&self) -> Int
}

pub trait Index[T, U] {
    type Output
    fn index(&self, index: U) -> &Output
}

}