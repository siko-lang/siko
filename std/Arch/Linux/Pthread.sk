module Arch.Pthread {

import Array

pub struct Mutex {
    _priv1: U64,
    _priv2: Array[U8, 40],

    @unsafe
    pub fn new() -> Mutex {
        Mutex(_priv1: 0.toU64(), _priv2: Array.uninitialized())
    }
}

pub struct MutexAttr {
    _priv1: U64,
    _priv2: Array[U8, 4],

    @unsafe
    pub fn new() -> MutexAttr {
        MutexAttr(_priv1: 0.toU64(), _priv2: Array.uninitialized())
    }
}

pub struct PthreadAttr {
    _priv1: I64,
    _priv2: Array[U8, 56],

    @unsafe
    pub fn new() -> PthreadAttr {
        PthreadAttr(_priv1: 0.toI64(), _priv2: Array.uninitialized())
    }
}

}