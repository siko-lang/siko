module Arch.Errno {

fn __errno_location() -> *I32 = extern "C"

@safe
pub fn getErrnoCode() -> Int {
    let errPtr: *I32 = __errno_location();
    let code: I32 = *errPtr;
    code.toInt()
}

@safe
pub fn setErrnoCode(code: Int) {
    let errPtr: *I32 = __errno_location();
    *errPtr = code.toI32();
}

}
