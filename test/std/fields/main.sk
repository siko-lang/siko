module Main {

class Person {
    name: String,
    address: Address,
}

class Address {
    street: String,
}

fn main() {
    let addr1 = Address("Main");
    let addr2 = Address("Main");
    let person1 = Person("John", addr1);
    let person2 = Person("Katie", addr2);
    assert(person1.address.street.eq(person2.address.street));
}

}