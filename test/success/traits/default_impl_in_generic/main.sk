module Main {

struct Foo {

}

trait Bar[T] {
    fn bar(&self) -> ()

    fn defaultbar(&self) -> () {

    }
}

instance Foo Bar[Foo] {
    fn bar(&self) -> () {

    }
}

fn generic[T: Bar[T]](t: T) {
    t.defaultbar();
}

fn main() {
    let f = Foo();
    f.bar();
    f.defaultbar();
    generic(f);
}

}