module Main {

struct Foo {
    value: Int
}

instance Drop[Foo] {
    fn drop(self) -> Self {
        println("Dropping Foo ${self.value}");
        self
    }
}

fn main() {
    {
        let first = Foo(1);
        let second = Foo(2);
    }
    {
        let second = Foo(2);
        let first = Foo(1);
    }
}

}