module Main {

struct Foo {

}

instance Drop[Foo] {
    fn drop(self) -> Self{
        println("dropping Foo");
        self
    }
}

fn sink(f: Foo) {
}

fn foo(f: Foo, b: Bool)  {
    if b {
        sink(f);
    }
}

fn main() {
    let f = Foo();
    foo(f, True);
    let f = Foo();
    foo(f, False);
}

}