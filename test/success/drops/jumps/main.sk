module Main {

struct Foo {

}

instance Drop[Foo] {
    fn drop(self) -> Self{
        println("dropping Foo");
        self
    }
}

fn sink[T](t: T) {
    println("sink");
}

fn main() {
    loop {
        {
            let f = Foo();
            if True {
                break;
            }
            sink(f);
        }
    }

}

}