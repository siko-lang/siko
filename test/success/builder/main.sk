module Main {

class Builder {
    value: Int

    fn new() -> Builder {
        Builder(0)
    }

    fn foo(self) -> Self {
        Builder(self.value.add(1))
    }

    fn bar(self) -> Self {
        Builder(self.value.add(2))
    }
}

fn main() {
    let mut builder = Builder.new();
    builder.foo();
    builder.bar();
    assert(builder.value.eq(3));

    let builder = Builder.new().foo().bar();
    assert(builder.value.eq(3));
}

}