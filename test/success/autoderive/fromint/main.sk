module Main {

import Convert

@derive(FromInt, PartialEq)
enum MyEnum {
    A,
    B,
    C
}

fn main() {
    let a : Option[MyEnum] = tryFrom(0);
    let b : Option[MyEnum] = tryFrom(1);
    let c : Option[MyEnum] = tryFrom(2);
    assert(a == Some(A));
    assert(b == Some(B));
    assert(c == Some(C));
}

}
