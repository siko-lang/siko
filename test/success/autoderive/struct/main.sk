module Main {

@derive(PartialEq, PartialOrd, Eq, Ord, Clone, Copy)
struct MyStruct[Q] {
    genericField: Q,
    someOther: Int,
}

fn main() {
    let e1 = MyStruct(1, 2);
    let e2 = MyStruct(3, 2);
    //test partial eq
    assert(e1.ne(&e2));
    //testpartial ord
    assert(e1.partialCmp(&e2) == Some(Less));
    // test ord
    assert(e1.cmp(&e2) == Less);
    let e2 = e1.clone();
}

}