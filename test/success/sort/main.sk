module Main {

import Rand

fn main() {
    let mut rand = Rand.new(0);
    let mut items = Vec.new();
    for i in 0..10000 {
        let value = rand.next();
        items.push(value);
    }
    let mut one = items.clone();
    let mut two = items.clone();
    one.sort();
    two.sort2();
    assert(one == two);
}

}