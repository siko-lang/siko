module Main {

import Rand

fn main() {
    let mut rand = Rand.new(0);
    for i in 0..10 {
        let value = rand.next();
        println("Random value: ${value}");
    }
    let mut count = 0;
    for value in rand {
        println("Random value: ${value}");
        count = count + 1;
        if count >= 10 {
            break;
        }
    }
}

}