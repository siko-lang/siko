module Main {

struct Foo {
    value: Int
}

fn other() {
    let mut items = Vec.new();
    items.push(4);
    let f = items.get(0);
    assert(f == 4);
}

fn nested() {
    let mut items : Vec[Vec[Foo]] = Vec.new();
    let mut first = Vec.new();
    first.push(Foo(4));
    let mut second = Vec.new();
    second.push(Foo(5));
    items.push(first);
    items.push(second);
}

fn main() {
    let mut items : Vec[Foo] = Vec.new();
    items.push(Foo(4));
    items.push(Foo(5));
    let f = items[0];
    assert(f.value == 4);
    let f = items[1];
    assert(f.value == 5);
    other();
    nested();
}

}