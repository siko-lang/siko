        Object(a)
    }

    fn set2(self, a: Int) -> (Self, Int) {
        (Object(a), a)
    }

    fn set3(self, a: Int) -> (Self, Int, Int) {
        (Object(a), a, a.add(1))
    }

    fn foo(self) {

    }
}

enum Adt {
    Foo(Int),
    Bar(Int)

    fn set(self, a: Int) -> Self {
        match self {
            Foo(v) -> Foo(v.add(a)),
            Bar(v) -> Bar(v.add(a)),
        }
    }
}

fn expectsTuple(t: ()) {

}

fn main() {
    let mut o1 = Object(1);
    assert(o1.value.eq(1));
    o1.foo();
    expectsTuple(o1.set(2));
    assert(o1.value.eq(2));
    assert(o1.set2(3).eq(3));
    assert(o1.value.eq(3));
    let t = o1.set3(4);
    assert(t.0.eq(4));
    assert(t.1.eq(5));
    assert(o1.value.eq(4));
    o1.foo();
    let mut adt = Foo(4);
    adt.set(5);
    match adt {
        Foo(x) -> assert(x.eq(9)),
        Bar(_) -> assert(False),
    };
