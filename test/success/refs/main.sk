module Main {

fn foo(s: &String) -> &String {
    assert(s.eq("foo"));
    assert(!s.eq("notfoo"));
    s
}

struct Object {
    s: &String
}

fn store(s: &String) -> Object {
    let o = Object(s);
    o
}

fn store2(s: String) -> Option[String] {
    let o = Some(s);
    o
}

fn store3(s: &String) -> Option[&String] {
    let o = Some(s);
    o
}

fn main() {
    let s = "foo".toString();
    let ref : &String = &&&&&&s;
    let ref2 = &ref;
    foo(s);
    foo(&s);
    let same = foo(&&&&s);
    assert(s.eq(same));
    let o = store(s);
    assert(s.eq(o.s));
    let o2 = store2(s.clone());
    match o2 {
        Some(s2) -> {
            assert(s.eq(s2));
        }
        None -> { assert(False) }
    };
    let o3 = store3(s);
    match o3 {
        Some(s3) -> {
            assert(s.eq(s3));
        }
        None -> { assert(False) }
    };
}

}