module Main {

struct Foo {

}

struct Foo2 {

}

trait Bar[T] {
    fn bar(self) -> ()
}

instance Default Bar[Foo] {
    fn bar(self) -> () {
        assert(True)
    }
}

instance Default2 Bar[Foo2] {
    fn bar(self) -> () {
        assert(False)
    }
}

fn main() {
    let f = Foo();
    f.bar(); // Bar[Foo] should be chosen
}

}